class Solution {
public:
    vector<int> buildArray(vector<int>& nums) {
        vector<int>ans; //or vector<int> ans(nums.size());
        ans.resize(nums.size()); //resize(n) – Resizes the container so that it contains ‘n’ elements.
        for(int i=0; i<nums.size(); i++){
            ans[i]=nums[nums[i]];
        }
        return ans;
        
    }
};
